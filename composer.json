{
    "name": "ggmm-one/portfolio",
    "type": "project",
    "description": "Project Portfolio Management",
    "keywords": [
        "portfolio",
        "project"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ggmm-one/model-helpers-for-laravel": "1.5.0",
        "ggmm-one/eloquent-hashids": "1.1.0",
        "laravel/framework": "^8.18",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.1",
        "facade/ignition": "^2.5",
        "fideloper/proxy": "^4.4",
        "timacdonald/rule-builder": "^4.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9",
        "nunomaduro/collision": "^5.1",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/providers",
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}